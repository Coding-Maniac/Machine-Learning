    "from typing import Tuple\n",
    "import math\n",
    "def normal_approximation_to_binomial(n:float ,p:float) -> Tuple[float, float]:\n",
    "    \"\"\"Return mu and sigma corresponding to m and p\"\"\"\n",
    "    mu = n * p\n",
    "    p = math.sqrt(p*(1-p)*n)\n",
    "    return mu,sigma\n"
